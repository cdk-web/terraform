{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash Debugger",
      "cwd": "${workspaceFolder}",
      "program": "${file}",
      "args": []
    },
    {
      "name": "Chrome Debugger",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:9000",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "serve"
    }
  ]
}
